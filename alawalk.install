## arg 1:  the new package version
post_install() {
  echo "If you wish to change any of the settings, run /usr/share/alawalk/install.sh as root."
  echo "Please don't use install.sh to uninstall or update this package."
  echo "You have pacman for that."
}

## arg 1:  the new package version
## arg 2:  the old package version
post_upgrade() {
  post_install
}

